        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn_preimgcancel"
        android:layout_below="@+id/relativeLayoutwrite"
        android:textColor="#FF0000"
        android:textSize="10pt" />

    <TextView
        android:id="@+id/txvprescoreoperation"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/mylist2"
        android:layout_alignLeft="@+id/textView_showpretableroomnum"
        android:layout_marginLeft="25dp"
        android:text="@string/scoreshow"
        android:textColor="#E0EFFF"
        android:textSize="10pt" />

</com.example.webview.tools.ResizeLayout>